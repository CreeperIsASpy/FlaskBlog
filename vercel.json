{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/python",
      "config": {
        "command": "bash build.sh"
      }
    },
    {
      "src": "index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.py"
    }
  ]
}